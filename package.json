{
  "name": "bera-youtube-mp3-downloader",
  "version": "1.0.0",
  "description": "YouTube MP3 Downloader API with Web Interface - Downloads real audio files (MB size)",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && echo 'Now install yt-dlp: pip install yt-dlp'",
    "install-all": "npm install && echo 'âœ… Node.js dependencies installed. Now run: pip install yt-dlp'",
    "clean": "rm -rf downloads/* && echo 'Cleaned downloads folder'",
    "health": "curl -s http://localhost:10000/health || echo 'Server not running'",
    "test-download": "curl -s 'http://localhost:10000/api/download/ytmp3?apikey=bera&url=https://youtu.be/dQw4w9WgXcQ&quality=128'"
  },
  "keywords": [
    "youtube",
    "mp3",
    "downloader",
    "api",
    "audio",
    "converter",
    "bera"
  ],
  "author": "Bera",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beradev/youtube-mp3-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/beradev/youtube-mp3-downloader/issues"
  },
  "homepage": "https://github.com/beradev/youtube-mp3-downloader#readme",
  "files": [
    "server.js",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
